	io.openems.edge.thermometer.api;version=latest,\
	io.openems.edge.relays.device.api;version=latest,\
	io.openems.edge.pwm.device.api;version=latest,\
	io.openems.edge.chp.device.api;version=latest,\
	io.openems.edge.meter.api;version=latest,\
	io.openems.edge.consolinno.leaflet.maindevice.api;version=latest,\

-testpath: \
	osgi.enroute.junit.wrapper;version=4.12, \
	osgi.enroute.hamcrest.wrapper;version=1.3

